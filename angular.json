{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xiv-raid-hub": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/xiv-raid-hub",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "sass"
              ]
            },
            "assets": [
              {"glob": "**/*","input": "src/assets", "output":  "./assets/"},
              {"glob": "**/*","input": "src/server", "output":  ".."}
            ],
            "styles": [
              {
                "input": "src/styles/themes/default/index.scss",
                "lazy": true,
                "bundleName": "default-theme"
              },
              {
                "input": "src/styles/themes/slate/index.scss",
                "lazy": true,
                "bundleName": "slate-theme"
              },
              {
                "input": "src/styles/themes/spacelab/index.scss",
                "lazy": true,
                "bundleName": "spacelab-theme"
              },
              "node_modules/pnotify/dist/PNotifyBrightTheme.css",
              "src/styles/styles.scss"
            ],
            "extractCss": true,
            "scripts": [
              "./node_modules/pnotify/dist/iife/PNotify.js",
              "./node_modules/pnotify/dist/iife/PNotifyButtons.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "xiv-raid-hub:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "xiv-raid-hub:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "xiv-raid-hub:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "analyze": {
          "builder": "@ngx-builders/analyze:analyze",
          "options": {
            "outputPath": "dist/xiv-raid-hub"
          }
        }
      }
    },
    "xiv-raid-hub-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "xiv-raid-hub:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "xiv-raid-hub:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "xiv-raid-hub",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss",
      "styleext": "scss"
    }
  }
}
